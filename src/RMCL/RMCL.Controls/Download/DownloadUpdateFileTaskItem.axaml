<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RMCL.Controls.Download.DownloadUpdateFileTaskItem">
    <StackPanel>
        <TextBlock Margin="0,5" Name="ProgressTextBox" TextTrimming="CharacterEllipsis">下载更新</TextBlock>
        <ProgressBar Margin="0,5" Name="DownloadProgress"></ProgressBar>
    </StackPanel>
</UserControl>
