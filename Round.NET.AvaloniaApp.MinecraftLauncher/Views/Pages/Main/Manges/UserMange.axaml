<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.MinecraftLauncher.Views.Pages.Main.Manges.UserMange">
    <Grid>
        <DockPanel HorizontalAlignment="Left" VerticalAlignment="Top">
            <Button Margin="5,5" Click="Button_OnClick">添加账户</Button>
            <Button Margin="5,5" Click="Button_OnClick1">刷新</Button>
        </DockPanel>
        <DockPanel HorizontalAlignment="Right" VerticalAlignment="Top">
            <Button Margin="5,5" Classes="accent" Name="GetUserSkin" Click="GetUserSkin_OnClick">获取正版账户皮肤</Button>
        </DockPanel>
        <ScrollViewer Margin="0,42,0,0">
            <StackPanel Name="Users">
                
            </StackPanel>
        </ScrollViewer>
        <DockPanel>
            
        </DockPanel>
    </Grid>
</UserControl>
